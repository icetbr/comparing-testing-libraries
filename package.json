{
  "name": "comparing-testing-libraries",
  "version": "1.0.0",
  "description": "Comparing the experience of using different testing libraries in javascript",
  "keywords": [
    "ava"
  ],
  "main": "index.js",
  "scripts": {
    "chokidar1": "chokidar '{test,src}/*.js' --initial",
    "mocha": "mocha test/employeeMochaTest.js",
    "mochaWatch": "mocha --reporter min --watch --inline-diffs -r chai/register-expect test/employeeMochaTest.js",
    "jest": "jest test/employeeJest.test.js",
    "jestWatch": "npm run jest -- --watch --runInBand --bail 1",
    "lab": "lab --silence test/employeeLabTest.js || true",
    "labWatchx": "npm run chokidar -- -c 'printf \"\\033c\"; lab --silence test/employeeLabTest.js'",
    "labWatch2": "nodemon lab --silence test/employeeLabTest.js",
    "labWatch3": "onchange -i 'src/**/*.js' 'test/**/*.js' -- npm run lab",
    "ava": "ava --serial test/employeeAvaTest.js",
    "avaWatch": "ava --serial --watch test/employeeAvaTest.js",
    "tape": "tape ./test/employeeTapeTest.js",
    "tapeReport": "tape ./test/employeeTapeTest.js | tap-difflet",
    "tapeWatch": "nodemon tape ./test/employeeTapeTest.js",
    "tapeWatchReport": "nodemon -x 'printf \"\\033c\"; tape ./test/employeeTapeTest.js | tap-difflet'",
    "tapeWatchReport2": "onchange 'src/**/*.js' 'test/**/*.js' -i -o 'printf \"\\033c\"; tape ./test/employeeTapeTest.js | tap-difflet'",
    "tapeWatchReport3": "chokidar '{test,src}/*.js' --initial -c 'printf \"\\033c\"; tape ./test/employeeTapeTest.js | tap-difflet'",
    "zora": "echo 'process.exit(0);' | node -r ./test/employeeZoraTest.js",
    "zoraReport": "echo 'process.exit(0);' | node -r ./test/employeeZoraTest.js | tap-summary",
    "zoraWatch": "nodemon -r ./test/employeeZoraTest.js",
    "zoraWatchReport": "chokidar --initial  '{test,src}/*.js' -c 'node -r ./test/employeeZoraTest.js | tap-difflet'",
    "zoraSingle": "node ./test/employeeZoraTest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icetbr/comparing-testing-libraries.git"
  },
  "author": "icetbr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/icetbr/comparing-testing-libraries/issues"
  },
  "homepage": "https://github.com/icetbr/comparing-testing-libraries#readme",
  "dependencies": {
    "@hapi/code": "^7.0.0",
    "@hapi/lab": "^21.0.0",
    "ava": "^2.4.0",
    "chai": "^4.2.0",
    "chokidar-cli": "^2.1.0",
    "even-more-min": "^1.0.1",
    "expect": "^24.9.0",
    "jest": "^24.9.0",
    "jest-silent-reporter": "^0.1.2",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.2",
    "onchange": "^6.1.0",
    "should": "^13.2.3",
    "tap": "^14.10.5",
    "tap-difflet": "^0.7.2",
    "tap-simple": "^0.3.1",
    "tap-summary": "^4.0.0",
    "tape": "^4.12.0",
    "unexpected": "^11.10.0",
    "zora": "^3.1.8"
  },
  "devDependencies": {
    "mocha-clearscreen-reporter": "^1.0.1"
  }
}
